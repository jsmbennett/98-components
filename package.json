{
  "name": "98-components",
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/98-components.js",
  "exports": {
    ".": {
      "import": "./dist/98-components.js"
    }
  },
  "scripts": {
    "dev": "pnpm run build:icons && vite serve --config vite.test.config.js",
    "build": "pnpm run build:icons && vite build",
    "build:icons": "node scripts/build-icons.js",
    "clean": "rimraf dist && rimraf dist-test && node scripts/build-icons.js --clean",
    "clean:docs": "rimraf docs",
    "clean:full": "pnpm run clean && rimraf node_modules",
    "build:test": "pnpm run build:icons && vite build --config vite.build-test.config.js",
    "preview": "vite preview",
    "docs": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "98.css": "^0.1.20"
  },
  "devDependencies": {
    "jsdoc": "^4.0.5",
    "vite": "^5.0.0",
    "rimraf": "^6.1.2"
  },
  "packageManager": "pnpm@10.24.0"
}
